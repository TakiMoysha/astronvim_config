{
  "loggingconf": {
    "prefix": "loggingconf",
    "body": [
      "import logging",
      "logging.basicConfig(format=\"%(levelname)s: %(message)s\",level=logging.INFO)"
    ]
  },
  "logging-formatting-template": {
    "prefix": "logging-formatting-template",
    "body": "\"%(asctime)s | %(levelname)-8s | %(message)s\""
  },
  "comment-rapture": {
    "prefix": "#comment-rapture",
    "body": "# ==========================================================================================\n"
  },
  "comment-rapture-sign": {
    "prefix": "#comment-rapture-sign",
    "body": [
      "# =========================================================== ",
      "# $1 ",
      "# =========================================================== \n"
    ]
  },
  "demo-complex": {
    "prefix": "co",
    "body": [
      "T1=${1:$RANDOM}",
      "T3=${3:$1_${2:$1}}",
      "T2=$2"
    ]
  }
}
